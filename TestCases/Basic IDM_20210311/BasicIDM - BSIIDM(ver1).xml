    <informationUnit id="9afd725d-98d7-4c06-b4be-0fd6a1b96ec3" name="which structure will we use?" dataType="various" isMandatory="true" definition="The agreements listed below help ensure that every involved party will always be able to find&#xD;&#xA;and supply the right information in the right place.">
      <subInformationUnit>
        <informationUnit id="d0ca08f6-1969-4a7c-beae-4f21a1de7e73" name="file name" dataType="string" isMandatory="true" definition="To ensure that uniform and consistent naming&#xD;&#xA;is used for (discipline) models within the&#xD;&#xA;project">
          <textualExample description="&lt;Building&gt;_&lt;Discipline&gt;_&lt;Component&gt; " />
          <graphicalExample fullFilePath="image\ER3-1.PNG" description="" />
        </informationUnit>
        <informationUnit id="ab643502-d7de-40bf-9116-7143b0622651" name="local position and orientation" dataType="Coordinate" isMandatory="true" definition="The local position of the building is coordinated and close to the origin.&#xD;&#xA;&#xD;&#xA;tip: use a physical object as point of origin, positioned at 0.0.0., and also export this to IFC.&#xD;">
          <graphicalExample fullFilePath="image\ER3-2.PNG" description="" />
        </informationUnit>
        <informationUnit id="a0554655-f5df-45f3-bfb4-3833fbf2dd45" name="building storeys and naming" dataType="string" isMandatory="true" definition="Name building storeys only as ifcBuildingStorey-Name.&#xD;&#xA;&#xD;&#xA;Allocate all objects to the correct level.&#xD;&#xA;&#xD;&#xA;Within a project, ensure that all involved parties consistently use exactly the same&#xD;&#xA;naming, that can be numerically sortedwith a textual description.">
          <textualExample description="00 ground floor" />
          <textualExample description="01 first floor" />
          <graphicalExample fullFilePath="image\ER3-3.PNG" description="" />
        </informationUnit>
        <informationUnit id="4a8a852c-25de-4df1-8f51-cb3689d1b38f" name="correct use of entities" dataType="string" isMandatory="true" definition="Use the most appropriate type of BIM entity, both in the source application and the IFC entity.">
          <textualExample description="slab = ifcSlab, wall = ifcWall, beam = ifcBeam, column = ifcColumn, stair = ifcStair, door = ifcDoor etc." />
          <graphicalExample fullFilePath="Image\ER3-4.PNG" description="" />
        </informationUnit>
        <informationUnit id="89be3bad-9489-4fe7-9cca-1063feb4c90c" name="structure and naming" dataType="string" isMandatory="true" definition="Consistently structure and name objects.&#xD;&#xA;Correctly enter the object TYPE (ifcType, ifcObjectType or ifcObjectTypeOverride).&#xD;&#xA;Where applicable, also correctly enter the Name (ifcName or NameOverride).">
          <textualExample description="roof insulation, type: glass fiber" />
          <graphicalExample fullFilePath="Image\ER3-5.PNG" description="" />
        </informationUnit>
        <informationUnit id="0272f46c-ef5a-4491-b9cb-1628ec57b33c" name="classification system" dataType="string" isMandatory="true" definition="Apply the existing classification system used in the relevant country. ">
          <textualExample description="22.11" />
          <graphicalExample fullFilePath="Image\ER3-6.PNG" description="" />
        </informationUnit>
        <informationUnit id="bbee70e0-994c-4705-b2a4-c4997b7b1142" name="objects with correct materialization" dataType="string" isMandatory="true" definition="Allocate objects with a material description (ifcMaterial).">
          <textualExample description="limestone" />
          <graphicalExample fullFilePath="Image\ER3-7.PNG" description="" />
        </informationUnit>
        <informationUnit id="e7742fca-631a-46ad-91f2-4fb2bcaf16cd" name="duplicates and intersections" dataType="boolean" isMandatory="true" definition="There are no duplicates or intersections permitted. Make sure this is checked in IFC.">
          <graphicalExample fullFilePath="Image\ER3-8.PNG" description="" />
        </informationUnit>
        <informationUnit id="e77ifa8a-884d-65ci-72a2-5nw6aieq58di" name="learning to speak the same language is something we do together" dataType="various" isMandatory="true" definition="When naming objects, consider whether the name meets the following criteria. Double-check this, and know what information you are sharing.">
          <textualExample description="Significant Understandable Logical Insightful Consistent Recognizable"/>
        </informationUnit>
      </subInformationUnit>
    </informationUnit>
    <informationUnit id="9cc7db56-2da3-4c83-a076-421d4279dee6" name="how can we secure other/future object information?" dataType="various" isMandatory="true" definition="Object information is secured in the correct properties and property sets as defined in IFC.">
      <subInformationUnit>
        <informationUnit id="097b4f08-1333-404c-96c0-e60d990ec6e9" name="loadbearing" dataType="boolean" isMandatory="true" definition="Allocate objects, when applicable, with the property LoadBearing">
          <textualExample description="[True/False]" />
          <graphicalExample fullFilePath="image\ER4-1.PNG" description="" />
        </informationUnit>
        <informationUnit id="26d40988-027e-4255-aaf5-4e6d8f59ca24" name="is external" dataType="boolean" isMandatory="true" definition="Allocate objects, when applicable, with the&#xD;&#xA;property IsExternal">
          <textualExample description="[True/False]" />
          <textualExample description="both inner and outer faces of the façade&#xD;&#xA;have the property IsExternalTrue." />
          <graphicalExample fullFilePath="Image\ER4-2.PNG" description="" />
        </informationUnit>
        <informationUnit id="d6b99d8a-36fd-4397-a480-77896b6fc177" name="fire rating" dataType="various" isMandatory="false" definition="Allocate objects, when applicable,&#xD;&#xA;with the property FireRating.">
          <textualExample description="Apply the existing standard used in the&#xD;&#xA;relevant country." />
          <graphicalExample fullFilePath="Image\ER4-3.PNG" description="" />
        </informationUnit>
        <informationUnit id="7fb3464b-7b1d-4157-af19-b574850f2383" name="project specific" dataType="IFC property" isMandatory="false" definition="Define which IFC properties you are using&#xD;&#xA;for each specific project.">
          <graphicalExample fullFilePath="Image\ER4-4.PNG" description="" />
        </informationUnit>
      </subInformationUnit>
      <textualExample description="for beams, the properties FireRating, LoadBearing and IsExternal are part of the Pset_BeamCommon." />
      <graphicalExample fullFilePath="image\ER4-0-1.PNG" description="" />
      <graphicalExample fullFilePath="image\ER4-0-2.PNG" description="" />
    </informationUnit>
